{
  "name": "storybook-stencil",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/storybook-stencil/storybook-stencil.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "npm-run-all --parallel start:stencil start:storybook",
    "start:stencil": "stencil build --dev --watch --serve",
    "start:storybook": "start-storybook -p 6006",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@stencil/core": "^1.12.7",
    "@stencil/sass": "^1.3.1",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-centered": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-notes": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/html": "^5.3.18",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "typescript-eslint": "0.0.1-alpha.0",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "license": "MIT",
  "dependencies": {}
}
